#include <stdio.h>

int sum_count(int a,int n)
{
    int sum=0,count=a;
    while(n-->0)
    {
        sum+=count;
        count=count*10+a;
    }
    return sum;
}

int main(int argc,char argvv[])
{
    int sum=0,a=0,n=0;
    scanf("%d %d",&a,&n);
    sum=sum_count(a,n);
    printf("%d\n",sum);

    return 0;
}
